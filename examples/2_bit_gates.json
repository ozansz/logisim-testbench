{
    "$meta": {
        "allowed_components": "__all__"
    },
    "inputs": [
        "A",
        "B"
    ],
    "variables": {
        "a": "int($A)",
        "b": "int($B)"
    },
    "outputs": {
        "AND": "$a * $b",
        "OR": "$a + $b",
        "NOR": "($a + $b) != 0",
        "XOR": "($a + $b) % 2",
        "XNOR": "($a + $b) % 2 == 0"
    }
}