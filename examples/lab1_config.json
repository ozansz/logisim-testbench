{
    "inputs": [
        "A1",
        "A0",
        "B1",
        "B0"
    ],
    "variables": {
        "a": "$A1 | $A0",
        "b": "$B1 | $B0"
    },
    "outputs": {
        "X": "$a + $b > 2",
        "Y": "$a % $b == 1 if $b != 0 else $a == 1",
        "Z": "($a << $b) > ($b << $a)"
    }
}
